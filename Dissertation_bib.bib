	Issn = {0888-3270},
	Journal = {Mechanical Systems and Signal Processing},
	Keywords = {Adaptive command shaping, Input shaping, Overhead crane, Payload hoisting, Sway control, Wind disturbance},
	Pages = {157 - 172},
	Title = {Adaptive output-based command shaping for sway control of a 3D overhead crane with payload hoisting and wind disturbance},
	Url = {http://www.sciencedirect.com/science/article/pii/S0888327017302273},
	Volume = {98},
