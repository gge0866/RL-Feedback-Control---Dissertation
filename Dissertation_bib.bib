	Date-Modified = {2016-10-04 05:01:45 +0000},
	Journal = {Journal of Dynamic Systems, Measurement, and Control},
	Keywords = {control system synthesis;cranes;pendulums;shape control;stability;},
	Language = {English},
	Month = {May},
