	Issn = {1364-0321},
	Journal = {Renewable and Sustainable Energy Reviews},
	Keywords = {Power quality disturbances, Signal processing, Artificial intelligence, Optimization techniques, Feature extraction},
	Pages = {1650 - 1663},
	Title = {A comprehensive overview on signal processing and artificial intelligence techniques applications in classification of power quality disturbances},
	Url = {http://www.sciencedirect.com/science/article/pii/S1364032115007157},
	Volume = {51},
	Year = {2015},
