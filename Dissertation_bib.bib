	Issn = {0957-4158},
	Journal = {Mechatronics},
	Keywords = {Residual vibration control, Input shaping, Duffing nonlinearity, Optimization},
	Number = {2},
	Pages = {184 - 198},
	Title = {Design of command shapers for residual vibration suppression in Duffing nonlinear systems},
