	Keywords = {computerised numerical control;milling;vibration control;CNC controllers;micromilling;nonlinear command shaping;vibration reduction;Computer numerical control;Cutting tools;DC motors;Machining;Milling machines;Motion control;Optical materials;Shape control;Throughput;Vibrations},
	Month = {June},
