	Keywords = {compensation;cranes;damping;hoists;motion control;ships;active antipendulation concept;base motion;boom angles;cargo transfer control;closed kinematic chain;compensation;crane actuation capability;crane commands;dual-crane system;dynamic simulation;feedback control laws;filtering strategies;hoist lengths;inertial space;inverse kinematic control;marine environment;measured platform motion;measured ship motion;minimum norm solution;motion controlled platform;multiple ship-mounted cranes;own ship motion;passive antipendulation concept;payload swing damping;single-crane control systems;target ship motion;tension member restraints;Cranes;Equations;Kinematics;Marine vehicles;Mathematical model;Payloads;Vectors;Control design;kinematics;motion compensation;motion compensation.},
	Month = {Jan},
