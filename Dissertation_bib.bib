	Keywords = {dynamic programming;adaptive control;stability;convergence;nonlinear control systems;learning (artificial intelligence);aerospace control;optimal control;soft computing applications;optimal cost functional;nonlinear system;unknown dynamics;hard computing;Hamilton-Jacobi-Bellman equation;state trajectories;input affine system;input quadratic performance measure;radial basis function approximation;flight control;adaptive control;optimal control;nonlinear control;control law;real-time;stability constraint;convergence;adaptive dynamic programming algorithm;Dynamic programming;Cost function;Function approximation;Control systems;Stability;Convergence;Computer applications;Heuristic algorithms;Fuses;Nonlinear systems},
	Month = {May},
