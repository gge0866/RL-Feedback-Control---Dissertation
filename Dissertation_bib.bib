	Keywords = {aircraft;collision avoidance;trajectory control;trees (mathematics);3D space;3D trajectory planning;Dubins-like vehicles;RRT* algorithm;aerial vehicles;obstacle avoidance;optimal rapidly exploring random tree algorithm;shortest paths;Aerodynamics;Convergence;Missiles;Planning;Trajectory;Vehicles;Aerial robotics;complex environment;optimal control;optimal rapidly exploring random tree (RRT*) algorithm;path planning algorithm},
	Month = {May},
	Number = {3},
	Pages = {1116-1123},
	Title = {3-D Trajectory Planning of Aerial Vehicles Using RRT*},
	Volume = {25},
	Year = {2017}}

