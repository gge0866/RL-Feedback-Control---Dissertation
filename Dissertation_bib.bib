	Journal = {IEEE Transactions on Robotics},
	Keywords = {robot dynamics;trajectory control;cable tension constraints;dynamic point-to-point trajectory planning;equivalent passive mechanical system;natural frequencies;point-to-point motion;three-DOF cable-suspended parallel robot;three-degree-of-freedom cable-suspended parallel robots;Acceleration;Dynamics;Mechanical systems;Parallel robots;Trajectory;Cable-suspended parallel robots;dynamics;trajectory planning},
	Month = {Dec},
